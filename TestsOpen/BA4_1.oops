CLASS Main IS
    METHOD tryer(a : Integer ; b:Integer) IS BEGIN
	TRY 
	    WRITE '0' + a/(b-b);
        CATCH 0 DO
	    WRITE 'c';
	    WRITE 'a';
	    WRITE 't';
	    WRITE 'c';
	    WRITE 'h';
	    WRITE 'e';
	    WRITE 'd';
        END TRY
        WRITE '\n';
    END METHOD
    METHOD main IS BEGIN
	SELF.tryer(3 , 2);
    END METHOD
END CLASS
